using System.ComponentModel.DataAnnotations;
using System.ComponentModel.DataAnnotations.Schema;

namespace Krat1.Server.Models.Kratos
{
    public class AlertaStock
    {
        public int alerta_id { get; set; }
        public int producto_id { get; set; }
        public int punto_venta_id { get; set; }
        public string codigo_ciiu { get; set; }
        public int nivel_alerta { get; set; }
         public bool notificado { get; set; }
        [DataType(DataType.DateTime)]
        public  datetime fecha_notificacion{ get; set; }
        public int usuario_notificado_id { get; set; }
        [DataType(DataType.DateTime)]
        public  datetime creado_en{ get; set; }
    
    }
}
